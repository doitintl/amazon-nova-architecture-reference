# AWS Credentials
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1  # As of 2025-05, us-east-1 is the primary region for Nova Sonic

# Nova Sonic Configuration
NOVA_SONIC_VOICE_ID=tiffany  # Options: matthew, tiffany, amy

# WebRTC Configuration
STUN_SERVER=stun:stun.l.google.com:19302
# TURN_SERVER=turn:your-turn-server.com:3478
# TURN_USERNAME=your_username
# TURN_PASSWORD=your_password

# Server Configuration
HOST=localhost
PORT=8000

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=
# DYNAMODB_ENDPOINT=http://localhost:8000  # Uncomment for local DynamoDB

# Logging Configuration
LOG_LEVEL=INFO  # Options: TRACE, DEBUG, INFO, WARNING, ERROR
LOG_FILE_PATH=/var/log/nova-sonic/api.log  # Path to log file

# Hotel Booking API Configuration
RESTAURANT_BOOKING_API_URL=  # URL for the hotel booking API (e.g., https://api.example.com/hotel-bookings)